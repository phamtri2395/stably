{
  "name": "@stably/dashboard.api",
  "version": "0.0.0",
  "private": true,
  "description": "Stably dashboard api service",
  "engines": {
    "node": ">= 12.0.0",
    "yarn": ">= 1.19.0"
  },
  "keywords": [
    "dashboard",
    "api"
  ],
  "workspaces": {
    "nohoist": []
  },
  "scripts": {
    "clean": "rimraf coverage dist",
    "prebuild": "npm run clean",
    "build": "tsc -b tsconfig.json",
    "test": "jest --config jest.config.js",
    "test:cov": "jest --config jest.config.js",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "start": "node -r ./module-alias.js dist/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.9",
    "@nestjs/core": "^7.0.9",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/platform-fastify": "^7.0.9",
    "@nestjs/schedule": "^0.3.1",
    "@nestjs/swagger": "^4.5.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "fastify-swagger": "^2.5.1",
    "mongoose": "^5.9.11",
    "nestjs-config": "^1.4.6",
    "ramda": "^0.27.0",
    "read-pkg-up": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "ts-mongoose": "^0.0.21",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.9",
    "@stably/tools": "^0.0.0",
    "@types/jest": "^25.2.1",
    "@types/mongoose": "^5.7.14",
    "@types/node": "^13.13.4",
    "@types/ramda": "^0.27.4",
    "@types/supertest": "2.0.8",
    "eslint-import-resolver-typescript": "^2.0.0",
    "jest": "^25.4.0",
    "rimraf": "^3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
